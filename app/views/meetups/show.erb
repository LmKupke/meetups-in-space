<h1><%= @meetup.name %></h1>
<ul class="meetup">
  <li class="description"> <%= @meetup.description %></li>
  <li class="location"> <%= @meetup.location %></li>
  <li class="user"> <%= @meetup.user.username%></li>
</ul>

<h2>Attendees</h2>
<ul class="attendees">
  <% @attendees.each do |attendee| %>
    <li><img src="<%= attendee.user.avatar_url %>" height="42" width="42"> <%= attendee.user.username %></li>
  <% end %>
</ul>


<br>
<% if @current_user != @meetup.user %>
  <form method="post" action="/meetups/<%=@meetup.id%>/join">
    <input type="submit" name="join" value="Join">
  </form>
<% end %>
